{
  "name": "@wineworlds/nuxt-block",
  "version": "2.0.1",
  "private": false,
  "description": "This module provides a way to define blocks in Nuxt templates, allowing them to be dynamically overridden elsewhere. It is fully compatible with **Nuxt Layers**.",
  "author": {
    "name": "Kubilay Melnikov",
    "email": "melnikov@wineworlds.de",
    "url": "https://www.wineworlds.de"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "publish": "npm publish --access public",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.0",
    "scule": "^1.3.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.7.0",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.0",
    "@nuxt/test-utils": "^3.20.1",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.38.0",
    "nuxt": "^4.2.0",
    "typescript": "~5.9.3",
    "vitest": "^4.0.4",
    "vue-tsc": "^3.1.2"
  },
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903"
}
